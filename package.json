{
  "name": "try-cra-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "electron": "^1.7.10",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start:wait": "wait-on $ELECTRON_START_URL",
    "start:electron": "electron .",
    "start:app": "BROWSER=none react-scripts start",
    "start:dev": "run-s start:wait start:electron",
    "start": "cross-env ELECTRON_START_URL=http://localhost:3000/ run-p start:app start:dev",
    "build:app": "react-scripts build",
    "build:electron": "electron-packager ./ --platform=darwin --arch=x64 --out=out --icon=./icon.icns --overwrite",
    "build": "run-s build:app build:electron",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "electron-packager": "^10.1.1",
    "wait-on": "^2.1.0"
  }
}
